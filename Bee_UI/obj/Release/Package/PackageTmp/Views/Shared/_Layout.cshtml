                <script>
                    var sayac = false;
                    var kontrol = 0;
                    function tara() {
                        if (document.getElementById("qrkod").getAttribute("style") == "display:none;") {
                            document.getElementById("qrkod").setAttribute("style", "display:block;");
                            document.getElementById("qrkod").innerHTML = "<iframe id='frame' src='/home/QROkuma'></iframe>";
                            kontrol = 0;
                            sayac = true;
                            var myVar = setInterval(myTimer, 100);
                            function myTimer() {
                                if (kontrol == 0) {
                                    var bol = document.getElementById('frame').contentWindow.location.toString().split("=");
                                    if (bol[1] != undefined) {
                                        kontrol = 1;
                                        document.getElementById("qrkod").setAttribute("style", "display:none;");
                                        document.getElementById("qrkod").innerHTML = "";
                                        document.getElementById("kovan").setAttribute("style", "display:block; margin-left:" + ((this.innerWidth <= 768 ? 50 : (this.innerWidth > 768 && this.innerWidth <= 1024 ? 200 : (this.innerWidth > 1024 ? 350 : "")))) + "px; width:" + ((this.innerWidth <= 768 ? this.innerWidth / 1.4 : (this.innerWidth > 768 && this.innerWidth <= 1024 ? this.innerWidth / 2 : (this.innerWidth > 1024 ? this.innerWidth / 5 : "")))) + "px;");
                                    }
                                }
                            }
                        }
                        else {
                            document.getElementById("qrkod").setAttribute("style", "display:none;");
                            document.getElementById("qrkod").innerHTML = "";
                            kontrol = 1;
                        }
                    }
                        function kapat()
                        {
                            document.getElementById("kovan").setAttribute("style", "display:none;");
                            document.getElementById("kovanim").setAttribute("style", "display:none;");
                        }
