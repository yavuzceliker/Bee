                        <script type="text/javascript">

                            var bugun = new Date("@DateTime.Now.ToString("MM.dd.yyyy hh:mm:ss") ");
                            var ilkbaharbasla = new Date(bugun.getFullYear(), 2, 15);
                            var ilkbaharbitir = new Date(bugun.getFullYear(), 4, 25);
                            var balbasla = new Date(bugun.getFullYear(), 4, 26);
                            var balbitir = new Date(bugun.getFullYear(), 8, 25);
                            var hasatbasla = new Date(bugun.getFullYear(), 8, 26);
                            var hasatbitir = new Date(bugun.getFullYear(), 9, 10);
                            var bakimbasla = new Date(bugun.getFullYear(), 9, 11);
                            var bakimbitir = new Date(bugun.getFullYear(), 10, 1);
                            var gun;
                            var kalangun;
                            var ortalama;
                            if (bugun >= ilkbaharbasla && bugun <= ilkbaharbitir) {
                                gun = ((ilkbaharbitir - ilkbaharbasla) / 1000 / 60 / 60 / 24);
                                kalangun = (ilkbaharbitir - bugun) / 1000 / 60 / 60 / 24;
                                ortalama = ((gun - kalangun) * 100) / gun;
                                document.getElementById("ilkbahar").setAttribute("aria-valuenow", ortalama);
                                document.getElementById("ilkbahar").setAttribute("style", "width:" + ortalama + "%;");
                                document.getElementById("ilkbahar").innerHTML = "<span>%" + ortalama.toFixed(2) + " Tamamlandı.</span>";
                                document.getElementById("kalangoster").innerText = "Dönemin bitmesine " + Math.round(kalangun) + " gün kaldı. Sonraki dönem bal-oğul dönemi.";
                            }
                            else if (bugun > ilkbaharbitir) {
                                document.getElementById("ilkbahar").setAttribute("aria-valuenow", "100");
                                document.getElementById("ilkbahar").setAttribute("style", "width:100%;");
                                document.getElementById("ilkbahar").innerHTML = "<span>%100 Tamamlandı.</span>";
                            }



                            if (bugun >= balbasla && bugun <= balbitir) {

                                gun = ((balbitir - balbasla) / 1000 / 60 / 60 / 24);
                                kalangun = (balbitir - bugun) / 1000 / 60 / 60 / 24;
                                ortalama = ((gun - kalangun) * 100) / gun;
                                document.getElementById("bal").setAttribute("aria-valuenow", ortalama);
                                document.getElementById("bal").setAttribute("style", "width:" + ortalama + "%;");
                                document.getElementById("bal").innerHTML = "<span>%" + ortalama.toFixed(2) + " Tamamlandı.</span>";
                                document.getElementById("kalangoster").innerText = "Dönemin bitmesine " + kalangun + " gün kaldı. Sonraki dönem hasat dönemi.";
                            }
                            else if (bugun > balbitir) {
                                document.getElementById("bal").setAttribute("aria-valuenow", "100");
                                document.getElementById("bal").setAttribute("style", "width:100%;");
                                document.getElementById("bal").innerHTML = "<span>%100 Tamamlandı.</span>";
                            }

                            if (bugun >= hasatbasla && bugun <= hasatbitir) {

                                gun = ((hasatbitir - hasatbasla) / 1000 / 60 / 60 / 24);
                                kalangun = (hasatbitir - bugun) / 1000 / 60 / 60 / 24;
                                ortalama = ((gun - kalangun) * 100) / gun;
                                document.getElementById("hasat").setAttribute("aria-valuenow", ortalama);
                                document.getElementById("hasat").setAttribute("style", "width:" + ortalama + "%;");
                                document.getElementById("hasat").innerHTML = "<span>%" + ortalama.toFixed(2) + " Tamamlandı.</span>";
                                document.getElementById("kalangoster").innerText = "Dönemin bitmesine " + kalangun + " gün kaldı. Sonraki dönem sonbahar dönemi.";
                            }
                            else if (bugun > hasatbitir) {
                                document.getElementById("hasat").setAttribute("aria-valuenow", "100");
                                document.getElementById("hasat").setAttribute("style", "width:100%;");
                                document.getElementById("hasat").innerHTML = "<span>%100 Tamamlandı.</span>";
                            }

                            if (bugun >= bakimbasla && bugun <= bakimbitir) {

                                gun = ((bakimbitir - bakimbasla) / 1000 / 60 / 60 / 24);
                                kalangun = (bakimbitir - bugun) / 1000 / 60 / 60 / 24;
                                ortalama = ((gun - kalangun) * 100) / gun;
                                document.getElementById("sonbahar").setAttribute("aria-valuenow", ortalama);
                                document.getElementById("sonbahar").setAttribute("style", "width:" + ortalama + "%;");
                                document.getElementById("sonbahar").innerHTML = "<span>%" + ortalama.toFixed(2) + " Tamamlandı.</span>";
                                document.getElementById("kalangoster").innerText = "Dönemin bitmesine " + kalangun + " gün kaldı. Sonraki dönem ilkbahar dönemi.";
                            }
                            else if (bugun > bakimbitir) {
                                document.getElementById("sonbahar").setAttribute("aria-valuenow", "100");
                                document.getElementById("sonbahar").setAttribute("style", "width:100%;");
                                document.getElementById("sonbahar").innerHTML = "<span>%100 Tamamlandı.</span>";
                            }









                        </script>
