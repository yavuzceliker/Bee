                <script type="text/javascript">
                    var kontrol = 0;
                    function tara() {
                        if (document.getElementById("qrkod").getAttribute("style") == "display:none;") {
                            kontrol = 0;
                            var myVar = setInterval(myTimer, 100);
                            document.getElementById("qrkod").innerHTML = "<iframe id='frame' src='/Home/QROkuma'></iframe>";
                            var iframe = document.getElementById('frame');
                            var innerDoc = (iframe.contentDocument) ? iframe.contentDocument : iframe.contentWindow.document;
                            document.getElementById("qrkod").setAttribute("style", "display:block;position:fixed;");
                            document.getElementById("frame").setAttribute("style", "width:300px;height:" + innerDoc.getElementById("preview").offsetHeight + "px;");

                            function myTimer() {
                                if (kontrol == 0) {
                                    var iframe = document.getElementById('frame');
                                    var innerDoc = (iframe.contentDocument)? iframe.contentDocument : iframe.contentWindow.document;
                                    if (innerDoc.getElementById("qrkodgelen").value != "") {
                                        kontrol = 1;
                                        document.getElementById("qrkod").setAttribute("style", "display:none;");
                                        document.getElementById("qrkod").innerHTML = "";
                                        verigetir(innerDoc.getElementById("qrkodgelen").value);


                                    }
                                }
                            }
                        }
                        else {
                            document.getElementById("qrkod").setAttribute("style", "display:none;");
                            document.getElementById("qrkod").innerHTML = "";
                            kontrol = 1;
                        }
                    }

                    function verigetir(veri) {
                         var Veri = { Veri: veri}
                                        $.ajax(
                                            {
                                                url: '@Url.Action("QRKod", "Home")',
                                                type: 'POST',
                                                dataType: 'json',
                                                data: Veri,
                                                success: function (data) {
                                                    if (data != "error") {

                                                        document.getElementById("detay").setAttribute("style", "visibility:visibled; z-index:100000;");

                                                        document.getElementById("yersiragelen").innerText = data[0];
                                                        document.getElementById("v-home").innerHTML = data[1];
                                                        document.getElementById("v-profile").innerHTML = data[2];
                                                        document.getElementById("v-messages").innerHTML = data[3];
                                                    }
                                                    else {
                                                        alert("Kovan Sistemde Bulunamadı.");
                                                    }
                                                }
                                            });

                    }

                    function kapat() {
                        document.getElementById("detay").setAttribute("style", "visibility:hidden;");
                        document.getElementById("kovanim").setAttribute("style", "display:none;");
                    }
                </script>


